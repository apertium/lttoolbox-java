  <rule comment="REGULO: DE PRN PP (de li dirita -> told by him)">
     <pattern>
       <pattern-item n="de"/>
       <pattern-item n="c_prpers"/>
       <pattern-item n="c_vb_pp"/>
     </pattern>
     <action>
        <call-macro n="firstWord">
