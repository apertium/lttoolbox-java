                 <clip pos="3" side="tl" part="a_gen" link-to="3"/>
                 <clip pos="3" side="tl" part="a_nbr" link-to="4"/>
                 <clip pos="3" side="tl" part="lemq"/>
               </lu>
             </chunk>
           </out>
        </when>
        <otherwise>
         <out> <!-- montra, rilativa k.a. pronomoj: estas nek persono nek genro -->
           <chunk name="pron">
