  <rule comment="REGULO: PP DE PRN (dirita de li -> told by him)">
     <pattern>
       <pattern-item n="c_vb_pp"/>
       <pattern-item n="de"/>
       <pattern-item n="c_prn"/>
     </pattern>
     <action>
        <call-macro n="firstWord">
          <with-param pos="1"/>
        </call-macro>
       <call-macro n="imp_inf"><with-param pos="1"/></call-macro>
       <call-macro n="set_vrb2"><with-param pos="1"/></call-macro>
        <out>
         <chunk name="vblex" case="caseFirstWord">
           <tags>
             <tag><lit-tag v="SV"/></tag>
              <tag><var n="vrb2"/></tag>
              <tag><clip pos="1" side="tl" part="a_tns"/></tag>
              <tag><lit-tag v="PD"/></tag> <!-- ni aldonas la spacon por eventuala aldono de pers en t2x -->
              <tag><lit-tag v="ND"/></tag> <!-- ni aldonas la spacon por eventuala aldono de nbr en t2x -->
            </tags>
            <lu>
              <clip pos="1" side="tl" part="lemh"/>
              <clip pos="1" side="tl" part="a_vrb"/>
              <clip pos="1" side="tl" part="a_tns"/>
              <lit-tag v="4"/> <!-- ni aldonas la spacon por eventuala aldono de pers en t2x -->
              <lit-tag v="5"/> <!-- ni aldonas la spacon por eventuala aldono de nbr en t2x -->
              <clip pos="1" side="tl" part="lemq"/>
            </lu>
          </chunk>
          <b pos="2"/>
          <chunk name="pr" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="PREP"/></tag>
            </tags>
            <lu>
              <lit v="by"/><lit-tag v="pr"/>
            </lu>
          </chunk>
          <b pos="1"/>
        </out>
      <choose>
        <when>
          <test>
            <in caseless="yes"><clip pos="3" side="sl" part="lem"/><list n="prn_kun_persono_kaj_genro"/></in>
          </test>
          <choose>
            <when>
              <test>
                <equal caseless="yes"><clip pos="3" side="tl" part="a_prn"/><lit-tag v="prn.subj"/></equal>
              </test>
              <let><clip pos="3" side="tl" part="a_prn"/><lit-tag v="prn.obj"/></let>
            </when>
          </choose>
           <out> <!-- persona pronomo: estas persono kaj genro -->
             <chunk name="pron">
               <tags>
                 <tag><lit-tag v="SN"/></tag>
                 <tag><clip pos="3" side="tl" part="a_prs"/></tag>
                 <tag><clip pos="3" side="tl" part="a_gen"/></tag>
                 <tag><clip pos="3" side="tl" part="a_nbr"/></tag>
                 <tag><clip pos="3" side="sl" part="a_cas"/></tag>
