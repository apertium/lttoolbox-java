<section-def-macros>

<!-- Por rigardi, ĉu la blanko havas aŭ ne havas formaton.
     Tiu makroo estas necesa en la reguloj, en kiuj malaperas vorto por decidi,
     ĉu la blanko de la vorto devas esti forigita aŭ konservita.
     Se ĝi havas formaton, necesas konservi ĝin; se ne, indas viŝi,
     por ke ne aperu du sinsekvaj blankoj.

     Prenita de "f_bcond" apertium-en-es/apertium-en-es.es-en.t1x -->
    <def-macro n="sendu_blankon_se_havas_formaton2" npar="2">
      <choose>
        <when>
          <test>
            <not><equal><b pos="1"/><lit v=" "/></equal></not>
          </test>
          <out>
            <b pos="1"/><!--
            <chunk name="sendu_blankon_se_havas_formaton">
              <tags><tag><lit-tag v="xx"/></tag></tags>
              <lu>
                <b pos="1"/>
              </lu>
            </chunk>-->
          </out>
        </when>
      </choose>
    </def-macro>

<!--
^dark<adj><sint>$
^dark<adj><sint><comp>$
^dark<adj><sint><sup>$

^malluma<adj><sg><nom>$
^pli<preadv>$ ^malluma<adj><sg><nom>$
^plej<preadv>$ ^malluma<adj><sg><nom>$

-->

<!-- Ĉi tiu makroo akceptas adv kaj adj kiel respektive unuan kaj duan parametrojn kaj analizas, ĉu temas
pri konstruo el la tipo 'pli granda', kiu estas sinteza en la angla -->
<def-macro n="adv_adj" npar="2">
<choose><!--  c="Ĉu estas pli aŭ plej? Ekz: 'tre bona'" -->
<when>
  <test>
  <in caseless="yes"><clip pos="1" side="sl" part="lemh"/><list n="l_pli_plej"/></in>
  </test>
   <choose><!--  c="Ĉu estas sinteza adjektivo? Ekz pli bona → better(sint)(comp)" -->
     <when>
    <test>
    <in caseless="yes"><clip pos="2" side="tl" part="lemh"/><list n="adj_kun_sint"/></in>
    </test>
      <call-macro n="sendu_blankon_se_havas_formaton2"><with-param pos="1"/><with-param pos="2"/></call-macro>
    <choose><!--  c="Ĉu pli aŭ plej? Metu (comp) aŭ (sup) en variablo" -->
    <when>
      <test>
      <equal caseless="yes"><clip pos="1" side="sl" part="lem"/><lit v="pli"/></equal>
      </test>
      <let><var n="adv_adjectiu"/><concat>
        <clip pos="2" side="tl" part="lem"/><lit-tag v="adj.sint.comp"/>
      </concat></let>
    </when>
    <otherwise><!--  c="ne estis 'pli' do devas esti 'plej'" -->
      <let><var n="adv_adjectiu"/><concat>
        <clip pos="2" side="tl" part="lem"/><lit-tag v="adj.sint.sup"/>
      </concat></let>
    </otherwise>
     </choose>
    </when>
    <otherwise><!--  c="ne estis sint - la variablo devas enhavi ambaŭ vortojn" -->
      <let><var n="adv_adjectiu"/><concat>
        <clip pos="1" side="tl" part="whole"/>
        <lit v="$"/><b pos="1"/><lit v="^"/>
        <clip pos="2" side="tl" part="lemh"/>
        <clip pos="2" side="tl" part="a_adj"/>
        <clip pos="2" side="tl" part="lemq"/>
      </concat></let>
    </otherwise>
  </choose>
</when>
  <otherwise><!--  c="ne estis pli aŭ plej - la variablo devas enhavi ambaŭ vortojn" -->
      <let><var n="adv_adjectiu"/><concat>
        <clip pos="1" side="tl" part="whole"/>
        <lit v="$"/><b pos="1"/><lit v="^"/>
        <clip pos="2" side="tl" part="lemh"/>
        <clip pos="2" side="tl" part="a_adj"/>
        <clip pos="2" side="tl" part="lemq"/>
      </concat></let>
    </otherwise>
</choose>
</def-macro>

<!-- rigardas, ĉu temas pri nepersona verbo kaj preparas "it" por aldono -->
<def-macro n="nepersona" npar="1">
<choose>
  <when>
    <test>
      <and>
        <in caseless="yes"><clip pos="1" side="sl" part="lemh"/><list n="nepersonaj_verboj"/></in>
        <not><equal><clip pos="1" side="sl" part="a_nepersonaj_tempoj"/><lit v=""/></equal></not>
      </and>
    </test>
  <let><var n="it"/><concat>
<!--    <lit v="^pron"/><lit-tag v="SN.p3.nt.sg.nom.nepersona"/><lit v="{^prpers"/><lit-tag v="prn.subj.2.3.4"/><lit v="$}$ "/>-->
    <lit v="^pron"/><lit-tag v="SN.p3.nt.sg.nom"/><lit v="{^prpers"/><lit-tag v="prn.subj.2.3.4"/><lit v="$}$ "/>
  </concat></let>
  </when>
  <otherwise>
  <let><var n="it"/><lit v=""/></let>
  </otherwise>
</choose>
</def-macro>


<!-- konvertas imperativon al infinitivo -->
<def-macro n="imp_inf" npar="1">
  <choose>
