</def-macro>


<!-- difinas la tipon vrb2 de la verbo  -->
<def-macro n="set_vrb2" npar="1">
  <choose>
    <when>
      <test>
          <equal><clip pos="1" side="tl" part="lemh"/><lit v="be"/></equal>
      </test>
      <let><var n="vrb2"/><lit-tag v="be"/></let>
    </when>
    <when>
      <test>
          <equal><clip pos="1" side="tl" part="lemh"/><lit v="can"/></equal>
      </test>
      <let><var n="vrb2"/><lit-tag v="can"/></let>
    </when>
    <otherwise>
      <let><var n="vrb2"/><lit-tag v="vbreg"/></let>
    </otherwise>
  </choose>
</def-macro>

    <def-macro n="firstWord" npar="1">
      <choose>
        <when>
          <test>
            <equal>
              <clip pos="1" side="sl" part="a_np_acr"/>
              <lit v=""/>
            </equal>
          </test>
          <choose>
            <when>
              <test>
                <equal> 
                  <var n="EOS"/>
                  <lit v="true"/>
                </equal> 
              </test>
              <modify-case>
                <clip pos="1" side="tl" part="lem"/>
                <lit v="aa"/>
              </modify-case>
              <let>
                <var n="caseFirstWord"/>
                <lit v="Aa"/>
              </let>
            </when>
            <otherwise> 
              <let>
                <var n="caseFirstWord"/>
                <lit v="aa"/>
              </let>
            </otherwise>
          </choose>
        </when>
        <otherwise>
          <let>
            <var n="caseFirstWord"/>
            <lit v="aa"/>
          </let>
        </otherwise>
      </choose>
      <let>
        <var n="EOS"/>
        <lit v="false"/>
      </let>
    </def-macro>

  </section-def-macros>


